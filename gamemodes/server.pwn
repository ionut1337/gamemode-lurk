			SetPVarInt( playerid, "gun", 30 );
			SetPVarInt( playerid, "slot", GetWeaponSlot( 30 ) );
			SetPVarInt( playerid, "gunPrice", Price );
			SetPVarInt( playerid, "gunMats", 150 );
