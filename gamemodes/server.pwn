{
	CheckPlayerAction( playerid );
	if( playerVariables[ playerid ][ pJob ] != 5 )				
		return SendClientMessage( playerid, COLOR_GREY, "You have to be an Arms Dealer!" );

	new Player, Weapon[ 25 ], Price;
