	nearByMessage( playerid, COLOR_PURPLE, NBM_DIST, "* %s has thrown %s out of their vehicle.", username( playerid ), username( Player ) );
	InsertCMDLog( GetPlayerMysqlID(playerid), GetPlayerMysqlID(Player), "/eject", "N/A" );
	return true;
