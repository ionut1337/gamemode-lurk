		return false;

	//( playerid, 1052, 0.0, 0.0, 0.0 );
	//( Player, 1139, 0.0, 0.0, 0.0 );

	SendClientMessagef( Player, COLOR_YELLOW, "SMS from %s (%d): %s", username( playerid ), playerVariables[ playerid ][ pPhoneNumber ], pText );
