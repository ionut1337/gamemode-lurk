	if( playerVariables[ playerid ][ pUndercover ] )
		ShowPlayerNameTagForPlayer( forplayerid, playerid, false );
	else if( !playerVariables[ playerid ][ pUndercover ] && playerVariables[ playerid ][ pFaction ] == 6 )
