alias:l("leaderchat");
CMD:l( playerid, params[] )
{
	if( playerVariables[ playerid ][ pFactionRank ] == 7 || playerVariables[ playerid ][ pAdminLevel ] > 0 )
	{
		new lText[ 256 ];
